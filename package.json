{
  "name": "pulseflow",
  "version": "1.0.0",
  "description": "AI-native scraping engine that monitors web signals and triggers LLM-summarized alerts",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "inngest:dev": "npx inngest-cli dev",
    "dev:all": "concurrently --names \"next,inngest,prisma\" --prefix-colors \"cyan,magenta,yellow\" \"npm run dev\" \"npm run inngest:dev\" \"npm run db:studio\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricocatford/pulseflow.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/ricocatford/pulseflow/issues"
  },
  "homepage": "https://github.com/ricocatford/pulseflow#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.2",
    "@tabler/icons-react": "^3.36.1",
    "cheerio": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "inngest": "^3.49.3",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "nuqs": "^2.8.6",
    "prisma": "^6.19.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "robots-parser": "^3.0.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
